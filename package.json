{
  "name": "silverclouding-theme",
  "version": "2.0.0",
  "description": "Repo with the tooling used in the SilverClouding Shopify theme.",
  "private": true,
  "author": "Blubolt",
  "license": "ISC",
  "scripts": {
    "watch:assets": "webpack --watch --env=dev --config webpack.dev.js",
    "build:assets": "webpack --config webpack.prod.js",
    "build:dev-assets": "webpack --config webpack.dev.js",
    "create:snippet": "node ./tools/createSnippet.js",
    "create:section": "node ./tools/createSection.js",
    "create:template": "node ./tools/createTemplate.js",
    "create:static-page": "node ./tools/copyStaticPage.js",
    "test:js": "jest ",
    "test:js:watch": "jest --watch",
    "test:schema": "node tools/schemaValidator.js",
    "test:cypress": "cd integration && npm run cypress:open",
    "test:cypress:ci": "cd integration && npm run cypress:ci",
    "subtree:pull": "git subtree pull --prefix theme theme-origin main --squash",
    "add:standards": "git subtree add --prefix coding-standards standards-origin main --squash",
    "update:standards": "git subtree pull --prefix coding-standards standards-origin main --squash",
    "a11y:lint": "node tools/a11y/lint.js",
    "scss:prettier": "npx prettier --write src/styles/**/*.scss",
    "scss:lint": "npx stylelint --fix src/styles/**/*.scss",
    "apply-remotes:ssh": "git remote add -f theme-origin git@github.com:SilverClouding/retreat-nature-theme.git && git remote add -f standards-origin git@github.com:blubolt/coding-standards.git",
    "apply-remotes:https": "git remote add -f theme-origin https://github.com/SilverClouding/retreat-nature-theme.git && git remote add -f standards-origin  https://github.com/blubolt/coding-standards.git",
    "start:live": "npm-run-all -p watch:assets serve:live",
    "serve:live": "shopify theme dev --store=retreat-nature.myshopify.com --path=theme --live-reload=full-page",
    "start:testing": "npm-run-all -p watch:assets serve:testing",
    "serve:testing": "shopify theme dev --store=retreat-testing.myshopify.com --path=theme --live-reload=full-page"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.8.4",
    "@babel/runtime-corejs3": "^7.8.4",
    "@shopify/prettier-plugin-liquid": "^1.4.4",
    "@types/json-parse-better-errors": "^1.0.0",
    "@types/ramda": "^0.26.43",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/experimental-utils": "^4.32.0",
    "@typescript-eslint/parser": "^3.7.0",
    "archiver": "^3.1.1",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.8",
    "babel-loader": "^8.0.6",
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0",
    "colors": "^1.4.0",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "dotenv": "^8.1.0",
    "eslint": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-webpack-plugin": "^2.5.4",
    "finalhandler": "^1.1.2",
    "fs-extra": "^8.1.0",
    "glob": "^7.2.0",
    "html-webpack-plugin": "^5.3.2",
    "http": "0.0.0",
    "immer": "^9.0.16",
    "jest": "^29.7.0",
    "js-yaml": "^3.13.1",
    "jsdom": "^19.0.0",
    "jsonlint": "^1.6.3",
    "mini-css-extract-plugin": "1.6.0",
    "npm-run-all": "^4.1.5",
    "open": "^7.2.1",
    "postcss": "^8.0.1",
    "postcss-loader": "6.1.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^3.2.5",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.0",
    "sass": "^1.56.2",
    "sass-loader": "^12.6.0",
    "serve-static": "^1.14.1",
    "stylelint": "^15.0.0",
    "stylelint-stylistic": "^0.4.3",
    "stylelint-webpack-plugin": "^4.0.0",
    "typescript": "^3.8.2",
    "webpack": "^5.91.0",
    "webpack-cli": "4.7.2",
    "webpack-fix-style-only-entries": "0.6.1",
    "webpack-merge": "5.8.0",
    "webpack-remove-empty-scripts": "^0.7.1",
    "yargs": "^14.2.0"
  },
  "dependencies": {
    "@ramda/merge": "^0.26.1",
    "fork-ts-checker-webpack-plugin": "^5.0.11",
    "html2pdf.js": "^0.10.1",
    "interweave": "^13.1.0",
    "js-cookie": "^2.2.1",
    "json-parse-better-errors": "^1.0.2",
    "ngrok": "^5.0.0-beta.2",
    "ramda.mergedeepright": "^0.26.1",
    "react": "^16.10.1",
    "react-compound-slider": "^3.3.1",
    "react-dom": "^16.10.1",
    "react-intersection-observer": "^8.31.0",
    "swiper": "^11.0.0",
    "terser-webpack-plugin": "^5.3.1",
    "throttle-debounce": "^2.1.0",
    "use-debounce": "^5.2.1"
  },
  "browserslist": [
    "last 2 version",
    "> 3%"
  ],
  "jest": {
    "testMatch": [
      "**/?(*.)unit.test.js?(x)"
    ]
  }
}
